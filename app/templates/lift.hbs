<div class="container">
  <h3>{{name}}</h3>
  <table class="table">
    <tr>
      <th>Date</th>
      <th>Load</th>
      <th>Rep Scheme</th>
      <th>Notes</th>
    </tr>
  {{#each results}}
    <tr>
      <td>{{date}}</td>
      <td>{{load}}</td>
      <td>{{repScheme}}</td>
      <td>{{dash-for-null notes}}</td>
    </tr>
  {{/each}}
  </table>
</div>
